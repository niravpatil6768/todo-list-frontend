{
    "version": 2,
      "builds": [
        {
          "src": "src/index.html", 
        "use": "@vercel/static",
          "config": {
            "maxSize": "3kb"
          }
        }
      ],
      
      "rewrites": [
        { "source": "/(.*)", "destination": "/index.html" }
      ]
    
    }